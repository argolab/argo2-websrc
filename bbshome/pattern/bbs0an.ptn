<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=gb2312" />
<title>中山大学逸仙时空BBS</title>
<link href="templates/global.css" rel="stylesheet" type="text/css" />
<style>
  body{
      margin-right:font-size: 16px, monospace;
  }
</style>

</head>

<body>
<form name="form1" method="post" action="">
<div id="head">
  <div id="location">
    <p><img src="images/location01.gif" alt="" align="absmiddle" width="11" height="14"/><a href="bbssec">【{BBSNAME}】</a></p>        
    <!-- IF BOARD -->
    <p><img src="images/location02.gif" alt="" align="absmiddle" />【{SECNAME}】</p>
    <p><img src="images/location03.gif" alt="" align="absmiddle" width="37" height="14" />【{BOARD}】</p>
    <!-- ENDIF -->
    <!-- IF ANNOUNCE -->
    <p><img src="images/location03.gif" alt="" align="absmiddle" width="37" height="14" />【{ANNOUNCE}】</p>
  	<!-- ENDIF -->
  </div>
</div>
  <div id="board_manager"></div> 
  <table class="table" border="0" cellspacing="0" cellpadding="0">
    <tr class="tb_head"> 
      <td><img src="images/table_ul05.gif" width="18" height="25" /></td>
      <td align="right"><img src="images/table_ur.gif" width="19" height="25" /></td>
    </tr>
    <tr> 
      <td colspan="2"> <table width="100%" border="0" cellspacing="0" cellpadding="0" height="">
          <tr> 
            <td width="18"><img src="images/table-left2.gif" alt="" align="absmiddle" width="18" height="28" /></td>
            <td class="td2" width="35" align="center">序号</td>
            <td class="td2" width="65" align="center">类别</td>
            <td width="649" class="td2" align="left">标题</td>
            <td width="92" class="td2" align="center">作者</td>
            <td width="113" align="right" class="td2"> <div style="float:left;padding-top:7px;">最后更新</div>
              <img src="images/table_right2.gif" alt="" align="absmiddle" /></td>
          </tr>
          <!-- BEGIN mainform -->
          <tr> 
            <td colspan="2" class="td5">{INDEX}</td>
            <td align="center" class="td6">【{TYPE}】</td>
            <td class="td6">{TITLE}</td>     
            <td class="td6">&nbsp;{BM}&nbsp;</td>
            <td class="td7">{DATE}</td>
          </tr>
          <!-- END mainform -->
          <tr> 
            <td colspan="2" class="td5">*</td>
            <td align="center" class="td6">【衔接】</td>
            <td class="td6"><a href="javascript:history.go(-1)">返回上一层</a></td> 
            <td class="td6">　</td>
            <td class="td7">　</td> 
          </tr>

<td colspan="5" class="tb_head_left"> </td>
            <td class="tb_head_right"> </td>
        </table></td>
    </tr>
    <tr> 
      <td class="tb_bottom"><img src="images/table_bl.gif" alt=""/></td>
      <td  class="tb_bottom" align="right"><img src="images/table_br.gif" alt="" /></td>
    </tr>
  </table>

  <div id="footer_search"> <input name="search" type="text" />
    <img src="images/search.gif" alt="搜索" align="absmiddle" />
    </div>
</form>
</body>
</html>
